package ex01_first;

public class Test {
//메인함수(main) -> 파일을 실행하면 main 함수 안에 있는 코드가 실행된다.
// main 템플릿 -> main 작성 후 ctrl + spacebar
//	public static void main(String[] args) {
//		
//		
//	}
	public static void main(String[] args) {
		//명령이 끝날때마다 세미콜론 반드시 찍기
		System.out.println("Hello World");
		//sysout 작성 후 ctrl + spacebar
		System.out.println();
	}
}
